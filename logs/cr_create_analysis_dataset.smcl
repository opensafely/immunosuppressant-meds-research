{com}. 
. label define agegroup   1 "18-<40" ///
>                                                 2 "40-<50" ///
>                                                 3 "50-<60" ///
>                                                 4 "60-<70" ///
>                                                 5 "70-<80" ///
>                                                 6 "80+"
{txt}
{com}.                                                 
. label values agegroup agegroup
{txt}
{com}. 
. * Create binary age
. recode age min/69.999 = 0 ///
>            70/max = 1, gen(age70)
{txt}(39345 differences between age and age70)

{com}. 
. * Check there are no missing ages
. assert age < .
{txt}
{com}. assert agegroup < .
{txt}
{com}. assert age70 < .
{txt}
{com}. 
. * Create restricted cubic splines for age
. mkspline age = age, cubic nknots(4)
{txt}
{com}. 
